{
  "name": "spiritlink02",
  "version": "1.0.0",
  "description": "SpiritLink",
  "main": "index.js",
  "scripts": {
    "start": "cd api && npm start index.js",
    "client-install": "cd client && npm install",
    "client-start": "cd client && npm start",
    "dev": "concurrently \"npm run start\" \"npm run client-start\"",
    "prod": "npm run client-install && npm run start",
    "api-build": "cd api && npm install",
    "client-build": "cd client && npm install && npm run build",
    "build": "npm run api-build && npm run client-build"
  },
  "dependencies": {
    "express": "1.0.0",
    "concurrently": "^8.2.2"
  },
